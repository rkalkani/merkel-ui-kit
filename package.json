{
  "name": "@merklescience/ui",
  "version": "0.1.0",
  "description": "Merklescience ui kit for react",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "typing": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rimraf dist && npm run typecheck && rollup -c",
    "validate": "run-s format lint typecheck",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ --ext .ts,.tsx,.js,.jsx --cache --fix",
    "format": "prettier --write \"**/*.{ts,tsx,css,html,js,jsx,json}\"",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "npm run build-storybook",
    "build-storybook": "build-storybook",
    "tailwincss:build": "node bin/general-tailwind-theme.js && prettier --write src/stories/tailwind.js && tailwindcss -i src/index.css -o .storybook/tailwind.css",
    "publish": "run-s validate build && npm publish @merklescience/ui@$(cat package.json | jq -r \".version\")-$(git describe --always)",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/merklescience/ui-kit.git"
  },
  "keywords": [
    "merklescience",
    "ui",
    "components",
    "designsystem"
  ],
  "author": "Rahul Kalkani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/merklescience/ui-kit/issues"
  },
  "homepage": "https://github.com/merklescience/ui-kit#readme",
  "dependencies": {
    "clsx": "^1.2.1",
    "date-fns": "^2.29.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
